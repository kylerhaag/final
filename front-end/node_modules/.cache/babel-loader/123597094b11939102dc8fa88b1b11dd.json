,IAAA,GAAA,IAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KARA;AASA,IAAA,MATA,oBASA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA,IADA;AAEA,kBAAA,GAAA,EAAA,MAAA,CAAA,GAFA;AAGA,kBAAA,KAAA,EAAA,MAAA,CAAA,KAHA;AAIA,kBAAA,MAAA,EAAA,MAAA,CAAA;AAJA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,EAFA;AAQA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA,CAAA,IAAA;AARA;AAAA;;AAAA;AAAA;AAAA;AAUA,gBAAA,OAAA,CAAA,GAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KArBA;AAuBA,IAAA,QAvBA,sBAuBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,IAAA,gBAAA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA,IADA;AAEA,kBAAA,GAAA,EAAA,MAAA,CAAA,GAFA;AAGA,kBAAA,KAAA,EAAA,MAAA,CAAA,KAHA;AAIA,kBAAA,MAAA,EAAA,MAAA,CAAA;AAJA,iBAAA,CAFA;;AAAA;AAQA,gBAAA,MAAA,CAAA,QAAA;;AARA;AAAA;;AAAA;AAAA;AAAA;AAUA,gBAAA,OAAA,CAAA,GAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAnCA;AAoCA,IAAA,QApCA,sBAoCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,eAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA;AAHA;AAAA;;AAAA;AAAA;AAAA;AAKA,gBAAA,OAAA,CAAA,GAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KA3CA;AA4CA,IAAA,UA5CA,sBA4CA,KA5CA,EA4CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,UAAA,uBAAA,KAAA,CAAA,GAAA,EAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA,kDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KArDA;AAsDA,IAAA,aAtDA,yBAsDA,KAtDA,EAsDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,KAAA;AADA;AAAA;AAAA,uBAGA,KAAA,CAAA,GAAA,uBAAA,KAAA,CAAA,GAAA,GAAA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA,IADA;AAEA,kBAAA,GAAA,EAAA,MAAA,CAAA;AAFA,iBAAA,CAHA;;AAAA;AAQA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AATA,kDAUA,IAVA;;AAAA;AAAA;AAAA;AAYA,gBAAA,OAAA,CAAA,GAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KApEA;AAqEA,IAAA,SArEA,qBAqEA,IArEA,EAqEA;AACA,WAAA,KAAA,GAAA,IAAA;AACA;AAvEA;AA/BA,CAAA","sourcesContent":["\n\n  <template>\n  <div class=\"main\">\n    <div class=\"menu\">\n      <p></p>\n      <h2>\n        {{user.firstName}} {{user.lastName}}\n        <button @click=\"logout\">Logout</button>\n      </h2>\n    </div>\n    <div>\n      <h2>Build A Teddy!</h2>\n    </div>\n    <p>\n      <label for=\"name\">Teddy's Name</label>\n      <input id=\"name\" v-model=\"name\" type=\"text\" name=\"name\" />\n    </p>\n    <p>\n      <label for=\"age\">Teddy's Age</label>\n      <input id=\"age\" v-model=\"age\" type=\"number\" name=\"age\" min=\"0\" />\n    </p>\n\n    <p>\n      <label for=\"gender\">Teddy's Gender</label>\n      <select v-model=\"gender\">\n        <option>Male</option>\n        <option>Female</option>\n      </select>\n    </p>\n    <p>\n      <label for=\"image\">Image</label>\n      <select v-model=\"image\" name=\"image\">\n        <option v-for=\"image in images\" :key=\"image\">{{image}}</option>\n      </select>\n    </p>\n    <div v-if=\"teddys.length\">\n      <hr />\n      <h2>Your Teddys!</h2>\n    </div>\n    <p>\n      <!-- clidk = upload -->\n      <input v-on:click=\"addItem1\" type=\"submit\" value=\"Add a New Teddy\" />\n    </p>\n    <!-- Display teddys -->\n    <div class=\"home\">\n      <div>\n        <div v-for=\"teddy in teddys\" :key=\"teddy.name\">\n          <h3>This is {{teddy.name}}</h3>\n          <h3 v-if=\"teddy.gender == 'Female'\">She is {{teddy.age}} years old</h3>\n          <h3 v-else>He is {{teddy.age}} years old</h3>\n\n          <img alt :src=\"'/images/' + teddy.image\" />\n          <p></p>\n          <button @click=\"deleteItem(teddy)\">Remove Teddy</button>\n          <p></p>\n          <button @click=\"openEdits(teddy)\">Edit Teddy's characteristics</button>\n          <p></p>\n          <div v-if=\"edits == true\">\n            <label for=\"name\">Teddy's New Name</label>\n            <input v-model=\"name\" type=\"text\" placeholder=\"name\" />\n            <p></p>\n            <label for=\"age\">Teddy's New Age</label>\n            <input v-model=\"age\" type=\"number\" min=\"0\" />\n            <p></p>\n            <button @click=\"completeEdits(teddy)\">Complete Edits</button>\n          </div>\n        </div>\n      </div>\n      <p></p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Teddy from \"../views/Teddy.vue\";\n\nimport axios from \"axios\";\nexport default {\n  name: \"Myteddys\",\n  data() {\n    return {\n      images: [\n        \"Boy1.jpg\",\n        \"Boy2.jpg\",\n        \"Boy3.jpg\",\n        \"Girl1.jpg\",\n        \"Girl2.jpg\",\n        \"Girl3.jpg\"\n      ],\n      name: \"\",\n      file: null,\n      addItem: null,\n      teddys: [],\n      age: \"\",\n      image: \"\",\n      gender: \"\",\n      edits: false\n    };\n  },\n  computed: {\n    user() {\n      return this.$root.$data.user;\n    }\n  },\n  components: {},\n  created() {\n    this.getItems();\n  },\n  methods: {\n    async logout() {\n      try {\n        await axios.delete(\"/api/users\");\n        this.$root.$data.user = null;\n      } catch (error) {\n        this.$root.$data.user = null;\n      }\n    },\n    async upload() {\n      try {\n        let r2 = await axios.post(\"/api/teddy\", {\n          name: this.name,\n          age: this.age,\n          image: this.image,\n          gender: this.gender\n        });\n        this.addItem = r2.data;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n\n    async addItem1() {\n      try {\n        await axios.post(`/api/teddys`, {\n          name: this.name,\n          age: this.age,\n          image: this.image,\n          gender: this.gender\n        });\n        this.getItems();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async getItems() {\n      try {\n        const response = await axios.get(`/api/teddys`);\n        this.teddys = response.data;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async deleteItem(teddy) {\n      try {\n        await axios.delete(`/api/teddys/${teddy._id}`);\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async completeEdits(teddy) {\n      this.edits = false;\n      try {\n        await axios.put(`/api/teddys/${teddy._id}`, {\n          name: this.name,\n          age: this.age\n        });\n\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    openEdits(item) {\n      this.edits = true;\n    }\n  }\n};\n</script>\n\n\n<style scoped>\n.menu {\n  display: flex;\n  justify-content: left;\n  background-color: #343a40;\n  color: #fff5a5;\n}\n\n.menu h2 {\n  font-size: 16px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}